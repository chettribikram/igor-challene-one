{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "igorblocks/testimonial",
	"version": "0.1.0",
	"title": "Igor Testimonial",
	"category": "igorblocks",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"attributes": {
		"quote": {
			"type": "string",
			"source": "html",
			"selector": "p",
			"default": ""
		},
		"authorName": {
			"type": "string",
			"source": "html",
			"selector": "h3",
			"default": ""
		},
		"jobTitle": {
			"type": "string",
			"source": "html",
			"selector": "p",
			"default": ""
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "igor-challenge-one",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
